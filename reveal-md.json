{
  "theme": "white",
  "highlightTheme": "github",
  "assetsDir": "assets",
  "css": [
    "custom.css"
  ],
  "plugins": [
    "highlight",
    "notes",
    {
      "name": "math",
      "config": {
        "mathjax": "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-mml-chtml.js",
        "tex": {
          "inlineMath": [["$", "$"], ["\\(", "\\)"]],
          "displayMath": [["$$", "$$"], ["\\[", "\\]"]]
        }
      }
    }
  ],
  "revealOptions": {
    "markdown": {
      "gfm": true 
    }
  }
}